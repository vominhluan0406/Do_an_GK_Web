window.onscroll = function () {
    scrollFunction()
};

function scrollFunction() {
    if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
        document.getElementById("go-to-top").style.display = "block";
    } else {
        document.getElementById("go-to-top").style.display = "none";
    }
}

scrollFunction();

function getParamaterFromUrl(name) {
    var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
    if (results == null) {
        return null;
    }
    else {
        return decodeURI(results[1]) || 0;
    }
}

function ajaxEvent() {
    var fullData = false;
    var ajaxCalling = false;
    if ($('.section-news').length >= 1) {
        $(window).scroll(function () {
            if ($(window).scrollTop() + $(window).height() >= $(document).height() - 100) {
                if (ajaxCalling) return;
                if (fullData) return;
                ajaxCalling = true;
                var offset = $('.news').length + 1;
                var url = '/load-more-ajax?cat_name=' + topic + '&offset=' + offset;
                $.get(url, function (data, status) {
                    if (data && data.trim().length > 0) {
                        $('#' + topic + ' .list-news').append($(data));
                    } else {
                        fullData = true;
                    }
                    ajaxCalling = false;
                });
            }
        });
    }
}

function controlTopic() {
    var topic = getParamaterFromUrl('topic');
    if (topic) {
        $('html,body').animate({
                scrollTop: $("#" + topic).offset().top - 100
            },
            'slow');
    }
}
function customizeForGPC() {
    if(getParamaterFromUrl('embedGPC')){
        $('.detail-page .side-right').hide();
        $('.detail-page .side-left').css('width','100%');
        $('.detail-page .side-left').css('font-size','3.1rem');
        $('.detail-page .title-detail-news').css('font-size','4rem');
        $('header').hide();
        $('.detail-page').css('padding-top','6rem');
    }
}
$(document).ready(function () {
    controlTopic();
    ajaxEvent();
    $('#go-to-top').click(function () {
        $('html').animate({scrollTop: 0}, 500, 'swing', function () {
        });
    });
    $('.search input').keyup(function (e) {
        if (e.keyCode == 13) {
            var q = $(this).val().trim();
            if (q.length == 0) return;
            var url = 'https://cse.google.com/cse?cx=009551682254834596188:0rhxtkmoake&ie=UTF-8&q=' + encodeURIComponent(q) + '#gsc.tab=0&gsc.q=' + encodeURIComponent(q) + '&gsc.sort=';
            if ($(this).parents('.nav-menu').length > 0) {
                var q = $(this).parent().find('input').val().trim();
                if (q.length == 0) return;
                var url = 'https://cse.google.com/cse?cx=009551682254834596188:0rhxtkmoake&ie=UTF-8&q=' + encodeURIComponent(q) + '#gsc.tab=0&gsc.q=' + encodeURIComponent(q) + '&gsc.sort=';
                location.href = url;
            } else {
                var win = window.open(url, '_blank');
                if (win) {
                    //Browser has allowed it to be opened
                    win.focus();
                } else {
                    location.href = url;
                }
                $(this).parent().removeClass('active');
            }
            $(this).val('').blur();

        }
    });
    $('.search img').click(function () {
        var q = $(this).parent().find('input').val();
        $(this).parent().find('input').val('').blur();
        $(this).parent().toggleClass('active');
        if (q.length == 0) return;
        var url = 'https://cse.google.com/cse?cx=009551682254834596188:0rhxtkmoake&ie=UTF-8&q=' + encodeURIComponent(q) + '#gsc.tab=0&gsc.q=' + encodeURIComponent(q) + '&gsc.sort=';
        var win = window.open(url, '_blank');
        if (win) {
            //Browser has allowed it to be opened
            win.focus();
        } else {
            location.href = url;
        }
    });

    $('.menu-wrapper .search img').click(function () {
        var q = $(this).parent().find('input').val().trim();
        if (q.length == 0) return;
        var url = 'https://cse.google.com/cse?cx=009551682254834596188:0rhxtkmoake&ie=UTF-8&q=' + encodeURIComponent(q) + '#gsc.tab=0&gsc.q=' + encodeURIComponent(q) + '&gsc.sort=';
        location.href = url;
    });

    $('header .menu').click(function () {
        $('.nav-menu').css('left', 0);
    });

    $('.close-menu').click(function () {
        $('.nav-menu').css('left', '-86%');
    });



